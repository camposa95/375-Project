<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="236.0" prefWidth="328.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.PauseMenuController">
   <children>
      <Button fx:id="saveButton" layoutX="208.0" layoutY="181.0" mnemonicParsing="false" onAction="#saveButtonPressed" prefHeight="26.0" prefWidth="96.0" text="Save &amp; Exit" />
      <Button fx:id="closeButton" layoutX="24.0" layoutY="182.0" mnemonicParsing="false" onAction="#close" text="Close" />
      <Text fx:id="title" layoutX="126.0" layoutY="49.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Game Paused" />
      <ChoiceBox fx:id="languageSelector" layoutX="88.0" layoutY="128.0" onAction="#changeLanguage" prefWidth="150.0" />
      <Text fx:id="selectorHeader" layoutX="115.0" layoutY="104.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Change Language" />
   </children>
</AnchorPane>
