<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="331.0" prefWidth="489.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.YearOfPlentyController">
   <children>
      <Group layoutX="140.0" layoutY="128.0">
         <children>
            <RadioButton fx:id="lumber1" mnemonicParsing="false" text="Lumber" />
            <RadioButton fx:id="brick1" layoutY="20.0" mnemonicParsing="false" text="Brick" />
            <RadioButton fx:id="wool1" layoutY="40.0" mnemonicParsing="false" text="Wool" />
            <RadioButton fx:id="grain1" layoutY="60.0" mnemonicParsing="false" text="Grain" />
            <RadioButton fx:id="ore1" layoutY="80.0" mnemonicParsing="false" text="Ore" />
         </children>
      </Group>
      <Group layoutX="272.0" layoutY="127.0">
         <children>
            <RadioButton fx:id="lumber2" mnemonicParsing="false" text="Lumber" />
            <RadioButton fx:id="brick2" layoutY="20.0" mnemonicParsing="false" text="Brick" />
            <RadioButton fx:id="wool2" layoutY="40.0" mnemonicParsing="false" text="Wool" />
            <RadioButton fx:id="grain2" layoutY="60.0" mnemonicParsing="false" text="Grain" />
            <RadioButton fx:id="ore2" layoutY="80.0" mnemonicParsing="false" text="Ore" />
         </children>
      </Group>
      <Text fx:id="firstResourceText" layoutX="140.0" layoutY="119.0" strokeType="OUTSIDE" strokeWidth="0.0" text="First Resource" />
      <Text fx:id="secondResourceText" layoutX="270.0" layoutY="118.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Second Resource" />
      <Text fx:id="yearOfPlentyTitleText" layoutX="157.0" layoutY="72.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Year Of Plenty! Select Two Cards:" />
      <Button fx:id="selectButton" layoutX="220.0" layoutY="260.0" mnemonicParsing="false" onMouseClicked="#submitYOP" text="Select" />
   </children>
</AnchorPane>
